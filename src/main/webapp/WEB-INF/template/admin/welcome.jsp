<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html class="x-admin-sm">
<head>
    <meta charset="UTF-8">
    <title>仙踪派</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport"
          content="width=device-width,user-scalable=yes, minimum-scale=0.4, initial-scale=0.8,target-densitydpi=low-dpi"/>
    <link rel="stylesheet" href="../css/font.css">
    <link rel="stylesheet" href="../css/xadmin.css">
    <script src="../lib/layui/layui.js" charset="utf-8"></script>
    <script type="text/javascript" src="../js/xadmin.js"></script>
</head>
<body>
<div class="layui-fluid">
    <div class="layui-row layui-col-space15">
        <div class="layui-col-md12">
            <div class="layui-card">
                <div class="layui-card-body ">
                    <blockquote class="layui-elem-quote">
                        <c:if test="${sessionScope.adminUser != null}">
                            <span>欢迎您：</span>
                        </c:if>
                        <span class="x-red">${sessionScope.adminUser != null ? sessionScope.adminUser.loginName : '未登录'}</span>！
                    </blockquote>
                </div>
            </div>
        </div>
        <div class="layui-col-md12">
            <div class="layui-card">
                <div class="layui-card-header">系统信息</div>
                <div class="layui-card-body ">
                    <table class="layui-table">
                        <tbody>
                        <tr>
                            <th>系统版本</th>
                            <td>1.0</td>
                        </tr>
                        <tr>
                            <th>服务器地址</th>
                            <td>http://localhost:8080/travel_agency/</td>
                        </tr>
                        <tr>
                            <th>操作系统</th>
                            <td>Windows</td>
                        </tr>
                        <tr>
                            <th>运行环境</th>
                            <td>Apache Tomcat 8.5+</td>
                        </tr>
                        <tr>
                            <th>Java版本</th>
                            <td>1.8 +</td>
                        </tr>
                        <tr>
                            <th>MySQL版本</th>
                            <td>8.0.15</td>
                        </tr>
                        <tr>
                            <th>Spring版本</th>
                            <td>5.1.5</td>
                        </tr>
                        <tr>
                            <th>Layui版本</th>
                            <td>2.5.7</td>
                        </tr>
                        <tr>
                            <th>jQuery</th>
                            <td>3.3.1</td>
                        </tr>
                        <tr>
                            <th>开发工具</th>
                            <td>IntelliJ IDEA 2020.1</td>
                        </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <div class="layui-col-md12">
            <div class="layui-card">
                <div class="layui-card-header">开发团队</div>
                <div class="layui-card-body ">
                    <table class="layui-table">
                        <tbody>
                        <tr>
                            <th>版权所有</th>
                            <td>戴国亮</td>
                        </tr>
                        <tr>
                            <th>开发者</th>
                            <td>戴国亮</td>
                        </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <style id="welcome_style"></style>
        <div class="layui-col-md12">
            <blockquote class="layui-elem-quote layui-quote-nm">感谢以上技术公司提供的技术支持！</blockquote>
        </div>
    </div>
</div>
</div>
</body>
</html>